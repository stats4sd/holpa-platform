{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The skeleton application for the Laravel framework.",
    "keywords": [
        "laravel",
        "framework"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.2",
        "althinect/filament-spatie-roles-permissions": "^2.2",
        "awcodes/filament-table-repeater": "^v3.1.0",
        "awcodes/shout": "^2.0",
        "better-futures-studio/filament-local-logins": "^1.2",
        "eightynine/filament-excel-import": "3.x-dev",
        "filament/filament": "^3.3.3",
        "haydenpierce/class-finder": "^0.5.3",
        "laravel/framework": "^11.9",
        "laravel/reverb": "^1.0",
        "laravel/telescope": "^5.2",
        "laravel/tinker": "^2.9",
        "maatwebsite/excel": "^3.1",
        "nesbot/carbon": "^3.8",
        "shiftonelabs/laravel-cascade-deletes": "^2.0",
        "spatie/laravel-ignition": "^2.9",
        "stats4sd/filament-odk-link": "*",
        "stats4sd/filament-team-management": "^1.0",
        "stats4sd/laravel-sql-views": "^1.2",
        "staudenmeir/belongs-to-through": "^2.16",
        "staudenmeir/eloquent-has-many-deep": "^1.20",
        "symfony/http-client": "^7.1",
        "symfony/mailgun-mailer": "^7.1",
        "valentin-morice/filament-json-column": "^1.4"
    },
    "require-dev": {
        "fakerphp/faker": "^1.23",
        "laravel/pint": "^1.13",
        "laravel/sail": "^1.26",
        "mockery/mockery": "^1.6",
        "nunomaduro/collision": "^8.0",
        "orangehill/iseed": "^3.0",
        "pestphp/pest-plugin-laravel": "^3.0",
        "pestphp/pest-plugin-livewire": "^3.0",
        "rector/rector": "^2.0",
        "spatie/laravel-ray": "^1.37"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi",
            "@php artisan filament:upgrade"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi",
            "@php -r \"file_exists('database/database.sqlite') || touch('database/database.sqlite');\"",
            "@php artisan migrate --graceful --ansi"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "php-http/discovery": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "repositories": {
        "stats4sd/filament-odk-link": {
            "type": "path",
            "url": "packages/filament-odk-link",
            "options": {
                "symlink": true
            }
        }
    }
}
